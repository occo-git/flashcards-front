{
  "consumer": {
    "name": "Frontend"
  },
  "provider": {
    "name": "UsersAPI"
  },
  "interactions": [
    {
      "description": "get logged in user info",
      "providerState": "User is authenticated",
      "request": {
        "method": "GET",
        "path": "/api/users/me",
        "headers": {
          "Authorization": {
            "match": "regex",
            "value": "Bearer .*"
          }
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "body": {
          "id": {
            "matcher": "uuid"
          },
          "username": "testuser",
          "level": "A1"
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "3.0.0"
    }
  }
}
